apiVersion: apps/v1
kind: Deployment
metadata:
  name: nginx
  namespace: nginx-vpa
spec:
  replicas: 2
  selector:
    matchLabels:
      app: nginx
  template:
    spec:
      metadata:
        labels:
          app: nginx
      containers:
      - name: nginx
        image: nginx
        ports:
        - containerPort: 80
